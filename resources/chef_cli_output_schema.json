    "type": "object",
    "properties": {
      "IfUserDecides": {
        "type": "object",
        "description": "Object containing user decision parameters"
      },
      "Outcome": {
        "type": "string",
        "description": "Description of the operation outcome"
      },
      "Warnings": {
        "type": ["array", "null", "string"],
